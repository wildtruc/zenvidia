#: .. from initial pot line
msgid ""
"$(cat <<-DEP\n"
"read -p \"Will you install them now ? (Y/N) :\" confirm\n"
"if [[ \"\\$confirm\" =~ [yY] ]]; then\n"
"echo -e \"\\n${grn}Depencies installation, please wait . . .${nc}\\n\"\n"
"$(pkg_install_cmd) ${deplist[*]}\n"
"sed -i \"s/resolve_dep=.*/resolve_dep=1/\" $script_conf\n"
"else\n"
"echo -e \"\\n${red}Those dependencies are mandatory for Zenvidia.${nc}\\n\"\n"
"echo -e \"If you don't want Zenvidia to install them automatically,\"\n"
"echo -e \"you need to use your packages manager instead\"\n"
"echo -e \"BYE !\"\n"
"fi\n"
"DEP\n"
")"
msgstr ""
"$(cat <<-DEP\n"
"read -p \"Voulez-vous les installer maintenant ? (O/N) :\" confirm\n"
"if [[ \"\\$confirm\" =~ [oO] ]]; then\n"
"echo -e \"\\n${grn}Installation des dépendances, patientez SVP . . .${nc}\\n\"\n"
"$(pkg_install_cmd) ${deplist[*]}\n"
"sed -i \"s/resolve_dep=.*/resolve_dep=1/\" $script_conf\n"
"else\n"
"echo -e \"\\n${red}Ces dépendances sont requises pour Zenvidia.${nc}\\n\"\n"
"echo -e \"Si vous ne voulez pas que Zenvidia les installe automatiquement,\"\n"
"echo -e \"vous devez utiliser le gestionnaire de paquets à la place\"\n"
"echo -e \"BYE !\"\n"
"fi\n"
"DEP\n"
")"

